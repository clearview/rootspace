  "license": "Unlicensed",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc --project tsconfig.json",
    "postbuild": "rsync -r ./src/templates/ ./dist/templates/",
    "dev": "ts-node --files src/index.ts",
    "dev:debug": "ts-node-dev --files --inspect=0.0.0.0:9229 --respawn src/index.ts",
    "queue:process": "ts-node-dev --inspect=0.0.0.0:9230 --respawn src/queueActivityProcessor.ts",
    "queue:cron": "ts-node-dev --inspect=0.0.0.0:9231 --respawn src/queueCronProcessor.ts",
    "y-websocket": "ts-node-dev --inspect=0.0.0.0:9232 --respawn src/yjs/run.ts",
    "tslint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "cli": "ts-node ./index.ts",
    "test": "jest"
  },
